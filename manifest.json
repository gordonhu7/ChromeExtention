{
  "manifest_version": 2,

  "name": "Be Productive",
  "description": "This extension helps track time spent on a webpage",
  "version": "1.0",

  "browser_action": {
    "default_icon":{
		"128": "simpleClock.png"
    },

    "default_title": "Click to get started!",   	
  	"default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
